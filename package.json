{
  "name": "quake",
  "displayName": "haptics-playground",
  "version": "1.1.2",
  "description": "An application that allows experimentation with haptic vibration.",
  "main": "haptics-playground-main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
